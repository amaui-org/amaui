{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "jsx": "react-jsx",
    "paths": {
      "@amaui/ui-react":["../amaui-ui-react/src"],
      "@amaui/ui-react/*":["../amaui-ui-react/src/*"]
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "test/**/*.test.js",
    "test/**/*.test.jsx",
    "test/**/*.test.ts",
    "test/**/*.test.tsx"
  ]
}
