{
  "name": "@amaui/ui-react",
  "version": "1.1.13",
  "description": "UI for React",
  "repository": "https://github.com/amaui-org/amaui.git",
  "author": "Lazar <lazareric2@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "ui",
    "react",
    "react-ui",
    "user-interface",
    "frontend",
    "front-end",
    "backend",
    "back-end",
    "web",
    "browser",
    "javascript",
    "js",
    "typescript",
    "node",
    "nodejs",
    "library",
    "amaui",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn lint && yarn type && cross-env ENV=react node ../../utils/js/build.js",
    "release": "yarn version --patch --no-git-tag-version && yarn build && cd ./build && npm publish",
    "lint": "tslint -c ../../tslint.json -p tsconfig.json",
    "type": "tsc --noEmit -p tsconfig.json",
    "cov": "cross-env NODE_ENV=test amaui-test",
    "test:pre": "pm2 update && pm2 delete all && pm2 start ../../utils/js/test/app.js && wait-on http://localhost:4000",
    "test:post": "pm2 delete all",
    "test": "yarn lint && yarn type && yarn build && yarn test:pre && yarn cov && yarn test:post"
  },
  "peerDependencies": {
    "@amaui/style-react": "^1.1.111",
    "react": "^18.1.0"
  },
  "dependencies": {
    "@amaui/date": "^1.0.11434",
    "@amaui/subscription": "^1.0.11151",
    "@amaui/utils": "^1.1.1"
  },
  "devDependencies": {
    "@amaui/style-react": "^1.1.111"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "build"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22"
  }
}